{
  "expo": {
    "name": "udacifitness",
    "slug": "udacifitness",
    "description": "App desenvolvido durante nanodegree React Developer da Udacity",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "./assets/icon.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcelomsoares.udacifitness"
    },
    "android": {
     "package": "com.marcelomsoares.udacifitness"
    }
  }
}